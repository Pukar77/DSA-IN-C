// Program to find the maximum element of array using concept of passing array through function
#include <stdio.h>
int maxsize(int a[], int len){
        int max= a[0];

    for(int i; i<len; i++){
        if(a[i]>max){
            max=a[i];
        }
    }
    printf("The maximum element of the array is %d", max);
    
}

int main() {
    int max,len;
    int a[]={44,39,56,43};
    
    len=sizeof a/ sizeof a[0];
    printf("The size of the array is %d\n", len);
    
    
    maxsize(a,len);
    
    
  

    return 0;
}
