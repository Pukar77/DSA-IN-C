
#include <stdio.h>
int sum(int b[],int len){
    int sum=0;
    for(int i=0;i<len;i++){
       sum=sum+b[i];
    }
           printf("The sum of the element is %d", sum);
}
int main() {
    int len;
    int a[]={1,2,3,4};
    len= sizeof a/ sizeof a[0];
    printf("The length of the array is %d\n", len);
    sum(a,len);
    return 0;
}


// NOTE: Next Method

#include <stdio.h>
int sum(int a[],int len){
    int sum=0;
    for(int i=0;i<len;i++){
       sum=sum+a[i];
    }
           printf("The sum of the element is %d", sum);
}
int main() {
    int len;
    int a[]={1,2,3,4};
    len= sizeof a/ sizeof a[0];
    printf("The length of the array is %d\n", len);
    sum(a,len);
    return 0;
}
